OpCapability PhysicalStorageBufferAddresses
OpCapability VulkanMemoryModel
OpCapability Shader
%1 = OpExtInstImport "GLSL.std.450"
OpMemoryModel PhysicalStorageBuffer64 Vulkan
OpEntryPoint Vertex %14 "vertex_main"
%float = OpTypeFloat 32
%3 = OpTypeFunction %float %float
%v3float = OpTypeVector %float 3
%8 = OpTypeFunction %float %v3float
%void = OpTypeVoid
%13 = OpTypeFunction %void
%_ptr_Function_float = OpTypePointer Function %float
%float_1 = OpConstant %float 1
%float_2 = OpConstant %float 2
%20 = OpConstantComposite %v3float %float_1 %float_1 %float_1
%4 = OpFunction %float None %3
%5 = OpLabel
OpReturnValue %float_1
OpFunctionEnd
%9 = OpFunction %float None %8
%10 = OpLabel
OpReturnValue %float_2
OpFunctionEnd
%14 = OpFunction %void None %13
%15 = OpLabel
%17 = OpVariable %_ptr_Function_float Function
%19 = OpVariable %_ptr_Function_float Function
%18 = OpFunctionCall %float %4 %float_1
OpStore %17 %18
%21 = OpFunctionCall %float %9 %20
OpStore %19 %21
OpReturn
OpFunctionEnd
