OpCapability PhysicalStorageBufferAddresses
OpCapability VulkanMemoryModel
OpCapability Shader
%1 = OpExtInstImport "GLSL.std.450"
OpMemoryModel PhysicalStorageBuffer64 Vulkan
OpEntryPoint Vertex %4 "vertex_main"
%void = OpTypeVoid
%3 = OpTypeFunction %void
%float = OpTypeFloat 32
%_ptr_Function_float = OpTypePointer Function %float
%v2float = OpTypeVector %float 2
%v3float = OpTypeVector %float 3
%float_1 = OpConstant %float 1
%11 = OpConstantComposite %v2float %float_1 %float_1
%float_2 = OpConstant %float 2
%13 = OpConstantComposite %v2float %float_2 %float_2
%17 = OpConstantComposite %v3float %float_1 %float_1 %float_1
%float_3 = OpConstant %float 3
%19 = OpConstantComposite %v3float %float_3 %float_3 %float_3
%4 = OpFunction %void None %3
%5 = OpLabel
%8 = OpVariable %_ptr_Function_float Function
%15 = OpVariable %_ptr_Function_float Function
%21 = OpVariable %_ptr_Function_float Function
%14 = OpDot %float %11 %13
OpStore %8 %14
%20 = OpDot %float %17 %19
OpStore %15 %20
%22 = OpLoad %float %8
%23 = OpLoad %float %15
%24 = OpFAdd %float %22 %23
OpStore %21 %24
OpReturn
OpFunctionEnd
